{
  "expo": {
    "name": "one-pickleball",
    "slug": "one-pickleball",
    "owner": "lddm4200",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo.png",
    "scheme": "onepickleball",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lddm4200.onepickleball",
      "buildNumber": "1",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "tel",
          "telprompt",
          "mailto",
          "https",
          "http"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png"
      },
      "permissions": [
        "android.permission.CALL_PHONE"
      ],
      "package": "com.lddm4200.onepickleball",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "tel"
          }
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          }
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "https"
          }
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "http"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Ứng dụng cần quyền truy cập thư viện ảnh để bạn có thể chọn ảnh đại diện và ảnh bìa.",
          "cameraPermission": "Ứng dụng cần quyền truy cập camera để bạn có thể chụp ảnh đại diện và ảnh bìa."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#00D9B5"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb69a13f-f75d-484d-9efc-799bb80e192c"
      }
    }
  }
}